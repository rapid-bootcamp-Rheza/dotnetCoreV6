@model dotnet_ef_core_6.DataContext.CustomerEntity
@{
    ViewBag.Title = "title";
    Layout = "_Layout";

}

<div>
    <h2>Detail Customer</h2>
    <table class="table table-bordered">
        <tr>
            <th>Customer Id</th>
            <td>@Model.CustomerId</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.CustomerName</td>
        </tr>
        <tr>
            <th>Gender</th>
            <td>@Model.Gender</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>@Model.Address</td>
        </tr>
        <tr>
            <th>City</th>
            <td>@Model.City</td>
        </tr>
        <tr>
            <th>Province</th>
            <td>@Model.Province</td>
        </tr>
    </table>

</div>
