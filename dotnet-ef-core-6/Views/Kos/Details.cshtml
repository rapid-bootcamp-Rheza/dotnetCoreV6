@model dotnet_ef_core_6.DataContext.KosEntity
@{
    ViewBag.Title = "title";
    Layout = "_Layout";

}

<div>
    <h2>Detail Kos</h2>
    <table class="table table-bordered">
        <tr>
            <th> Id</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.NamaPenyewa</td>
        </tr>
        <tr>
            <th>Gender</th>
            <td>@Model.Gender</td>
        </tr>
        <tr>
            <th>Status Perkawinan</th>
            <td>@Model.Status</td>
        </tr>
        <tr>
            <th>Harga Sewa per Bulan</th>
            <td>@Model.Sewa</td>
        </tr>
        <tr>
            <th>Jenis Kamar Kos</th>
            <td>@Model.JenisKos</td>
        </tr>
        <tr>
            <th>Waktu Tinggal per Bulan</th>
            <td>@Model.LamaSewa</td>
        </tr>
    </table>

</div>
