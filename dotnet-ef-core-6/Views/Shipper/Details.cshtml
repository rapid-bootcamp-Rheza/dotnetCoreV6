@model dotnet_ef_core_6.DataContext.ShipperEntity
@{
    ViewBag.Title = "title";
    Layout = "_Layout";

}

<div>
    <h2>Detail Shipper</h2>
    <table class="table table-bordered">
        <tr>
            <th> Id</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.NamaShipper</td>
        </tr>
        <tr>
            <th>Jenis Produk</th>
            <td>@Model.Product</td>
        </tr>
        <tr>
            <th>Region</th>
            <td>@Model.Region</td>
        </tr>
        <tr>
            <th>Harga shipper per kilo</th>
            <td>@Model.PriceShipper</td>
        </tr>
        <tr>
            <th>Jumlah Branch</th>
            <td>@Model.Branch</td>
        </tr>
    </table>

</div>
